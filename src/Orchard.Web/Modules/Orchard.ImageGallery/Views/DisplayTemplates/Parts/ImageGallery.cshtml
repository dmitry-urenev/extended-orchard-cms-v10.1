@using Orchard.ImageGallery.Helpers
@using System.Web.Mvc.Html
@{ Style.Require("ImageGallery"); }

@model Orchard.ImageGallery.ViewModels.ImageGalleryViewModel

<div class="image-gallery">
    <ul>
        @foreach (var image in Model.Images)
        {
            <li>
                <a href="@image.PublicUrl" title="@image.Caption">
                    @Html.Image(image)
                </a>            
            </li>
        }                     
    </ul>     
</div>



